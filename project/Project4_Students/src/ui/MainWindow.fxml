<VBox prefHeight="550.0" prefWidth="759.0" stylesheets="@Login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controller.MainController">
   <children>
      <HBox>
         <children>
            <ImageView fitHeight="94.0" fitWidth="285.0" pickOnBounds="true">
