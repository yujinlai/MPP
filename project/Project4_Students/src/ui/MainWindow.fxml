<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import ui.*?>

<VBox prefHeight="550.0" prefWidth="684.0" stylesheets="@Login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <Label prefHeight="85.0" prefWidth="275.0" text="Welcome  XXXXXXXX" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font size="34.0" />
               </font>
               <textFill>
                  <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                     <stops>
                        <Stop color="#7252a8" />
                        <Stop color="WHITE" offset="1.0" />
                     </stops>
                  </RadialGradient>
               </textFill>
            </Label>
            <ImageView fitHeight="94.0" fitWidth="383.0" pickOnBounds="true">
               <image>
                  <Image url="@library.jpg" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <FlowPane id="FuncBtnPane" prefHeight="53.0" prefWidth="497.0">
         <children>
            <Button fx:id="addMemberBtn" mnemonicParsing="false" text="Add Member">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="checkoutBookBtn" mnemonicParsing="false" text="Checkout Book">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="addCopyBtn" mnemonicParsing="false" text="Add Copy">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="addBookBookBtn" mnemonicParsing="false" text="Add Book">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="printRecordBtn" mnemonicParsing="false" text="Print Record">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="checkoutOverdueBtn" mnemonicParsing="false" text="Checkout Overdue">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
         <effect>
            <Blend />
         </effect>
      </FlowPane>
      <StackPane prefHeight="370.0" prefWidth="600.0">
      	 <children>
      		<AddMemberWindow fx:id="addMemberPane" />
      		<CheckoutBookWindow fx:id="checkoutBookPane" />
      		<AddCopyWindow fx:id="addCopyPane" />
      		<AddBookWindow fx:id="addBookPane" />
      		<PrintRecordWindow fx:id="printRecordPane" />
      		<CheckOverdueWindow fx:id="CheckOverduePane" />
      	</children>
      </StackPane>
      
   </children>
   
</VBox>
